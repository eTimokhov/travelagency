<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>AgencyRenderingServlet</servlet-name>
        <servlet-class>by.bsuir.et.controller.web.AgencyRenderingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgencyRenderingServlet</servlet-name>
        <url-pattern>/agency</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CustomerRenderingServlet</servlet-name>
        <servlet-class>by.bsuir.et.controller.web.CustomerRenderingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerRenderingServlet</servlet-name>
        <url-pattern>/customer</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TourRenderingServlet</servlet-name>
        <servlet-class>by.bsuir.et.controller.web.TourRenderingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TourRenderingServlet</servlet-name>
        <url-pattern>/tour</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HotelRenderingServlet</servlet-name>
        <servlet-class>by.bsuir.et.controller.web.HotelRenderingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HotelRenderingServlet</servlet-name>
        <url-pattern>/hotel</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EventRenderingServlet</servlet-name>
        <servlet-class>by.bsuir.et.controller.web.EventRenderingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventRenderingServlet</servlet-name>
        <url-pattern>/event</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FlightRenderingServlet</servlet-name>
        <servlet-class>by.bsuir.et.controller.web.FlightRenderingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FlightRenderingServlet</servlet-name>
        <url-pattern>/flight</url-pattern>
    </servlet-mapping>
</web-app>